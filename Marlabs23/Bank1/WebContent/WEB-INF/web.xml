<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>Bank1</display-name>
  <welcome-file-list>
    <welcome-file>html/personalInfo.html</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>personal</servlet-name>
  	<servlet-class>com.as.samples.PersonalInfoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>personal</servlet-name>
  	<url-pattern>/personal</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>contact</servlet-name>
  	<servlet-class>com.as.samples.ContactInfoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>contact</servlet-name>
  	<url-pattern>/contact</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>bank</servlet-name>
  	<servlet-class>com.as.samples.BankInfoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>bank</servlet-name>
  	<url-pattern>/bank</url-pattern>
  </servlet-mapping>
</web-app>